<aura:component implements="flexipage:availableForAllPageTypes" controller="lightningTreeComponentController">    
    <aura:attribute name="items" type="Object"/>
    <aura:attribute name="parentObj" type="String"/>
    <aura:attribute name="childObj" type="String" />
    <aura:attribute name="relationship" type="String" />

    <aura:attribute name="header" type="String" access="Global" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    
    <lightning:tree items="{!v.items}" header="{!v.header}"/>
</aura:component>

